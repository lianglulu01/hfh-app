\n    increment (context) {\n      context.commit('add');\n    },\n    //context称为上下文，指现在的mutations对象\n    change(context, value) {\n      //commit提交\n      context.commit('changeUsername', value);\n    }\n  },\n  modules: {\n  }\n})\n"]}]}